{
  "name": "vue-sdz",
  "version": "0.0.1",
  "files": [
    "dist/"
  ],
  "main": "dist/bundle.cjs.js",
  "module": "dist/index.esm.js",
  "private": false,
  "husky": {
    "hooks": {
      "pre-commit": "yarn build"
    }
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "yarn build && poi --serve examples/main.js",
    "postbuild": "poi",
    "test-helpers": "jest 'src/helpers'",
    "test-components": "jest 'src/components'",
    "test-e2e:run": "cypress run",
    "test-e2e:open": "cypress open 'tests/e2e/integration/**/*'",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.8",
    "@rollup/plugin-commonjs": "14.0.0",
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-plugin-unit-jest": "^4.5.13",
    "@vue/compiler-sfc": "3.0.2",
    "@vue/test-utils": "^2.0.0-rc.11",
    "autoprefixer": "^10.3.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "26.3.0",
    "babel-loader": "^8.2.2",
    "cypress": "^8.0.0",
    "cypress-rollup-preprocessor": "^0.6.2",
    "eslint": "^7.31.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-vue": "^7.14.0",
    "husky": "^7.0.0",
    "jest": "26.6.3",
    "node-sass": "^6.0.1",
    "poi": "^12.10.3",
    "postcss": "^8.3.5",
    "rollup": "^2.53.3",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-scss": "3",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "^1.36.0",
    "sass-loader": "10",
    "vue": "^3.1.5",
    "vue-jest": "5.0.0-alpha.5",
    "vue-loader": "^16.3.3"
  },
  "license": "MIT",
  "repository": "https://github.com/marlonfrontend/vue-sdz"
}
