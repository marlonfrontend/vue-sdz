    "@vue/composition-api": "^1.7.0",
    "@vue/test-utils": "^1.2.2",
    "autoprefixer": "^10.3.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "26.3.0",
    "babel-loader": "^8.2.2",
    "commitizen": "^4.2.4",
    "css-loader": "^6.2.0",
    "cypress": "^8.0.0",
    "cypress-rollup-preprocessor": "^0.6.2",
    "cz-coenvencional": "^1.0.8",
    "eslint": "^7.31.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-vue": "^7.16.0",
    "husky": "^7.0.0",
