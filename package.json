    "vite": "^3.0.0",
    "vite-plugin-glob": "^0.3.2",
    "vite-plugin-vue2": "^2.0.2",
    "vue": "2.6.14",
    "vue-jest": "^3.0.7",
    "vue-loader": "^15.9.8",
    "vue-router": "^3.5.3",
    "vue-template-compiler": "2.6.14",
    "vuex": "^3.6.2"
